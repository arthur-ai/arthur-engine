########################################################
## Arthur GenAI Engine Environment Variables
########################################################
# GPT service type ("Azure" or "OpenAI")
GENAI_ENGINE_OPENAI_PROVIDER=OpenAI

# GPT access
# Format: <model_name>::<base_url>::<api_key>
# If using OpenAI provider, leave the base_url part of the template empty unless
# you are using a proxy or service emulator (eg: OpenAI API compatible model)
GENAI_ENGINE_OPENAI_GPT_NAMES_ENDPOINTS_KEYS=model_name::::my_api_key

########################################################
## Arthur ML Engine Environment Variables
########################################################
# Enable fetching raw data to the platform in memory only for inference deep dive capabilities in the UI
FETCH_RAW_DATA_ENABLED=true

# Values that will be generated by the platform after registration
ARTHUR_CLIENT_ID=
ARTHUR_CLIENT_SECRET=
